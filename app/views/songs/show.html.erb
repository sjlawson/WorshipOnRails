<div class='container'>
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @song.title %>
</p>

<p>
  <strong>Content:</strong>
<div class='col-sm-6'>
<pre><%= @song.content %>
</pre>
</div>
</p>

<p>
  <strong>License:</strong>
  <%= @song.license %>
</p>

<p>
  <strong>Author:</strong>
  <%= @song.author %>
</p>

<p>
  <strong>User:</strong>
  <%= @song.user.email %>
</p>

<%= link_to 'Edit', edit_song_path(@song) %> |
<%= link_to 'Back', songs_path %>
</div>
